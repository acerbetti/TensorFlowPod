{
  "name": "TensorFlowPod",
  "version": "0.1.0",
  "license": "MIT",
  "summary": "Use Google TensorFlow as pod (unofficial)",
  "homepage": "https://github.com/acerbetti/TensorFlowPod",
  "authors": {
    "Stefano Acerbetti": "acerbetti@gmail.com"
  },
  "source": {
    "git": "https://github.com/acerbetti/TensorFlowPod.git"
  },
  "platforms": {
    "ios": "9.2"
  },
  "requires_arc": false,
  "ios": {
    "vendored_libraries": "TensorFlowPod/lib/*.a",
    "frameworks": "Accelerate",
    "header_mappings_dir": "tensorflow",
    "xcconfig": {
      "HEADER_SEARCH_PATHS": "\"${PODS_ROOT}/../../../TensorFlowPod/include\""
    }
  }
}
