{
  "name": "TensorFlowPod",
  "version": "0.1.0",
  "license": "MIT",
  "summary": "Use Google TensorFlow as pod (unofficial)",
  "homepage": "https://github.com/acerbetti/TensorFlowPod",
  "authors": {
    "Stefano Acerbetti": "acerbetti@gmail.com"
  },
  "source": {
    "git": "https://github.com/acerbetti/TensorFlowPod.git"
  },
  "platforms": {
    "ios": "9.2"
  },
  "requires_arc": false,
  "ios": {
    "vendored_libraries": "TensorFlowPod/lib/*.a",
    "frameworks": "Accelerate",
    "header_mappings_dir": "tensorflow",
    "public_header_files": [
      "tensorflow/tensorflow/core/**/*.h",
      "tensorflow/third_party/**/*.h",
      "tensorflow/**/Tensor",
      "tensorflow/tensorflow/contrib/makefile/downloads/**/*.h",
      "tensorflow/tensorflow/contrib/makefile/gen/proto/**/*.h"
    ]
  }
}
